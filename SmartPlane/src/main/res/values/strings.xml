    <string name="version">1</string>
    <string name="dbname">planedatadb</string>
    <string-array name="create">
        <item>
            create table rudder(
            id integer primary key autoincrement,
            timestamp int not null,
            rudder int not null
            )
        </item>
        <item>
            create table motor(
            id integer  primary key autoincrement,
            timestamp int not null,
            motor int not null
            )
        </item>
        <item>
            create table connection(
            id integer primary key autoincrement,
            timestamp int not null,
            connection boolean not null
            )
        </item>
    </string-array>
    <string name="insertRudderPrep">insert into rudder(timestamp,rudder) values(?,?)</string>
    <string name="insertMotorPrep">insert into motor(timestamp,motor) values(?,?)</string>
    <string name="insertConnectionPrep">insert into connection(timestamp,connection) values(?,?)</string>
    <string name="tableNameRudder">rudder</string>
    <string name="tableNameMotor">motor</string>
    <string name="tableNameConnection">connection</string>
